#config
set color
set rmstar
set implicitcd = verbose
set colorcat
set history = 1000000
set savehist = (1000000 merge)
set histfile = ~/.tcsh_history
unset autologout
set complete = enhance
bindkey "\t" complete-word-fwd
set autocorrect
set autoexpand
set fignore=(.psq .psi .psd .phr .pin .nin .nsq .nhr .fai .ann .pac)
set prompt = "%U@%m >> "
set rprompt="[%/]"
limit coredumpsize 0
set ignoreeof
set colorcat
setenv BLOCKSIZE G
setenv PAGER less
set notify
set nobeep
complete cd 'p/1/d/'
setenv LESS -r
setenv EDITOR emacs

#Alias
alias ta "tmux attach"
alias tw 'tmux list-windows'
alias screen '$HOME/bin/screen/src/screen'
alias lv '/home/soh.i/bin/lv451/src/lv'
alias ls 'ls -tF --color'
alias lf 'ls -FA'
alias grep 'grep --color'
alias emacs 'emacs -nw'
alias c 'clear'
alias rm '/home/soh.i/bin/trash.sh'
alias mv 'mv -i'
alias screenkill "ps ax | grep SCREEN | awk '{print $1}' | xargs kill"
alias finger '/home/soh.i/bin/finger'
alias sl 'ls'
alias ks 'ls'
alias kess 'less'
alias lc 'lv'
alias logout 'exit'

alias dstat-full 'dstat -Tclmdrn'
alias dstat-mem 'dstat -Tclm'
alias dstat-cpu 'dstat -Tclr'
alias dstat-net 'dstat -Tclnd'
alias dstat-disk 'dstat -Tcldr'

